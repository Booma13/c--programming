1)Sum and Difference
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cin>>a;
    cin>>b;
    cout<<a+b<<"\n";
    cout<<a-b;
    return 0;
}

2)Product and Division
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cin>>a;
    cin>>b;
    cout<<a*b<<"\n";
    cout<<a/b<<"\n";
    cout<<a%b;
    return 0;
}

3)Swapping two numbers
#include<iostream>
using namespace std;
int main()
{
    int a,b,c=0;
    cin>>a;
    cin>>b;
    c=a;
    a=b;
    b=c;
    cout<<a<<"\n";
    cout<<b;
    return 0;
}

4)Swapping two numbers without third variable
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cin>>a;
    cin>>b;
    a=a+b;
    b=a-b;
    a=a-b;
    cout<<a<<"\n";
    cout<<b;
    return 0;
}

5)Average Calculation
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float a,b,c,d,e,s;
    cin>>a>>b>>c>>d>>e;
    s=(a+b+c+d+e)/5;
    cout<<fixed<<setprecision(2)<<s;
    return 0;
}
6)Area Calculation
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int sq,lr,br;
    float rc;
    cin>>sq>>lr>>br>>rc;
    int as=sq*sq;
    int ar=lr*br;
    float ac=3.14*rc*rc;
    cout<<as<<"\n";
    cout<<ar<<"\n";
    cout<<fixed<<setprecision(2)<<ac;
    return 0;
}

7)Power of a number
#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    int x,n,z;
    cin>>x>>n;
    z=pow(x,n);
    cout<<z;
    return 0;
}

8)Alice in Wonderland
#include<iostream>
using namespace std;
int main()
{
    int n,a,b,c;
    cin>>n;
    a=n/10;
    b=n%10;
    c=a+b;
    cout<<"Alice must go in path-"<<c;
    return 0;
}

9)Simple Interest
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int p,n;
    float r,i;
    cin>>p>>n>>r;
    i=(p*n*r)/100;
    cout<<fixed<<setprecision(2)<<i;
    return 0;
}

10)3 idiots
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float x1,y1,x2,y2;
    float x,y;
    cin>>x1>>y1>>x2>>y2;
    x=(x1+x2)/2;
    y=(y1+y2)/2;
    cout<<"Binoy's house is located at ("<<fixed<<setprecision(1)<<x<<","<<fixed<<setprecision(1)<<y<<")";
    
    return 0;
}

11)spliting into teams
#include<iostream>
using namespace std;
int main()
{
    int n,t,tm,l;
    cin>>n>>t;
    tm=n/t;
    l=n%t;
    cout<<"The number of students in each team is "<<tm<<" and left out is "<<l;
    
    return 0;
}

12)Ternary Operator
#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    cout<<(n%2==0?"Even":"Odd");
    return 0;
}

13)Fencing The Ground
#include<iostream>
using namespace std;
int main()
{
    int l,b,rl;
    cin>>l>>b;
    rl=2*l+2*b;
    cout<<"Required length is "<<rl<<"m\n";
    cout<<"Required quantity of carpet is "<<l*b<<"sqm";
    
    return 0;
}

