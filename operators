23)Romans VS Greeks
#include<iostream>
using namespace std;
int main()
{
    int x,y,l;
    cin>>x;
    cin>>y;
    cin>>l;
    int d=x+(l/2);
    int f=y+(l/2);
    cout<<d<<" "<<f;
    return 0;
}

24)New Member in Team Flash
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float x1,x2,x3,y1,y2,y3,t1,t2;
    cin>>x1;
    cin>>y1;
    cin>>x2;
    cin>>y2;
    cin>>x3;
    cin>>y3;
    t1=(x1+x2+x3)/3;
    t2=(y1+y2+y3)/3;
    cout<<fixed<<setprecision(1)<<t1<<"\n";
    cout<<fixed<<setprecision(1)<<t2;
    return 0;
}

25)The Discount
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float i1,i2,d;
    cin>>i1;
    cin>>i2;
    cin>>d;
    float tp=i1+i2;
    float ds=tp-((tp*d)/100);
    float s=tp-ds;
    cout<<fixed<<setprecision(2)<<tp<<"\n";
    cout<<fixed<<setprecision(2)<<ds<<"\n";
    cout<<fixed<<setprecision(2)<<s;
    return 0;
}

26)Treasure Hunt
#include<iostream>
using namespace std;
int main()
{
    int n,fp,hp,f,h,pp,r,p;
    cin>>n;
    cin>>fp;
    cin>>hp;
    f=(n*fp)/100;
    h=((n-f)*hp)/100;
    r=n-(f+h);
    p=(r/3);
    cout<<f<<"\n";
    cout<<h<<"\n";
    cout<<p;
    
    return 0;
}

27)Binfon a Tribal
#include<iostream>
using namespace std;
int main()
{
    int n,y,w,yr,wr,d;
    cin>>n;
    y=n/365;
    yr=n%365;
    w=yr/7;
    wr=yr%7;
    d=wr;
    cout<<y<<"\n";
    cout<<w<<"\n";
    cout<<d;
    
    return 0;
}

28)The Baggings
#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    int x,y,a,b,c;
    cin>>x;
    cin>>y;
    a=pow((x-3),2);
    b=pow((y-4),2);
    c=sqrt(a+b);
    cout<<c;
    return 0;
}

29)Chill Factor
#include<iostream>
#include<iomanip>
#include<math.h>
using namespace std;
int main()
{
    float t,v,c;
    cin>>t;
    cin>>v;
    c=(35.74+(0.6215*t)+(0.4275*t-35.75)*pow(v,0.16));
    cout<<fixed<<setprecision(2)<<c;
    return 0;
}
