15)Profit Calculation
#include<iostream>
using namespace std;
int main()
{
    int x,a,b,c,s,w,p;
    cin>>x>>a>>b;
    s=x*a;
    c=x*b;
    p=s-(c+100);
    cout<<"The profit obtained is Rs."<<p;
    
    return 0;
}

16)Arithmetic Operator
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cin>>a;
    cin>>b;
    cout<<a <<" + "<< b <<" = " <<a+b<<"\n";
    cout<<a <<" - "<< b << " = " <<a-b<<"\n";
    cout<<a <<" * "<< b <<" = " <<a*b<<"\n";
    cout<<a <<" / "<< b <<" = " <<a/b<<"\n";
    cout<<a <<" % "<< b <<" = " <<a%b;
    
    return 0;
}

17)Business Proposal
#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float p,r,y;
    cin>>p;
    cin>>r;
    cin>>y;
    float a=(p*r*y)/100;
    float m=a+p;
    float d=(0.02*a);
    float s=m-d;
    cout<<fixed<<setprecision(2)<<a<<"\n";
    cout<<fixed<<setprecision(2)<<m<<"\n";
    cout<<fixed<<setprecision(2)<<d<<"\n";
    cout<<fixed<<setprecision(2)<<s;
    return 0;
}

